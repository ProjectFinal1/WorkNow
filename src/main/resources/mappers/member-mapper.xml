<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SignUp">
	<resultMap id="resultMember" type="SignUp">
		<id property="memberid" 	   column="MEMBER_ID" />
  		<result property="memberpass"  column="MEMBER_PASS"/>
  		<result property="membername"  column="MEMBER_NAME"/>
  		<result property="memberphone" column="MEMBER_PHONE"/>
  		<result property="memberemail" column="MEMBER_EMAIL"/>
  		<result property="membertype"  column="MEMBER_TYPE"/>
  		<result property="memberdate"  column="MEMBER_DATE"/>
  		<result property="memberblack" column="MEMBER_BLACK"/>  	
	</resultMap>
	
	<insert id="signup" parameterType="SignUp">
 		INSERT INTO MEMBER_INFO VALUES(#{memberid}, #{memberpass}, #{membername}, #{memberphone},
			#{memberemail}, 0,SYSDATE, 0)		
  	</insert>
</mapper>